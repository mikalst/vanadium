{
    "name": "Vanadium",
    "description": "The simplest navigation tool for Chrome",
    "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "64": "icons/icon64.png" 
    },
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "js": ["links.js"]
        },
        {
            "matches": ["https://*/*"],
            "js": ["scrolling.js"]
          }
      ]
  }